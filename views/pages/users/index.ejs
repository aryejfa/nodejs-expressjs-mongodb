<!DOCTYPE html>
<html lang="en">
  <%- include('../../components/header') %>
  <body>
    <%- include('../../components/navbar') %>
    <h2>Data Users <%=users.length%></h2>
    <ul>
      <% if(users.length > 0){ %> <% users.forEach((user) => { %>
      <li>
        <a href="/users/show/<%=user.id%>"> <%=user.name.toUpperCase() %> </a>
      </li>
      <% }) %> <% }else{ %>
      <li>Data empty</li>
      <% } %>
    </ul>
    <%- include('../../components/footer') %>
  </body>
</html>
